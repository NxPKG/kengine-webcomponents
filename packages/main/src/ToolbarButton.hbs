<kengine-button
	class="kengine-tb-button kengine-tb-item"
	id="{{this.id}}"
	style="{{this.styles}}"
	icon="{{this.icon}}"
	?icon-end="{{this.iconEnd}}"
	tooltip="{{this.tooltip}}"
	accessible-name="{{this.accessibleName}}"
	accessible-name-ref="{{this.accessibleNameRef}}"
	.accessibilityAttributes="{{this.accessibilityAttributes}}"
	design="{{this.design}}"
	?disabled="{{this.disabled}}"
	?hidden="{{this.hidden}}"
	data-kengine-external-action-item-id="{{this._id}}"
	data-kengine-stable="{{this.stableDomRef}}"
	.refItemId="{{this._id}}"
>
		{{this.text}}
</kengine-button>